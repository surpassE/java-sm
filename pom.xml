<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.sirding</groupId>
	<artifactId>java-sm</artifactId>
	<packaging>war</packaging>
	<version>0.0.1-SNAPSHOT</version>
	<name>java-sm Maven Webapp</name>
	<url>http://maven.apache.org</url>

	<description>
	  	整合框架
	  		Spring4+,
	  		Spring security 4+, 
	  		oauth2.0+, 
	  		Mybatis 3.3+, 
	  		freemarker2.3+
	  	MQ
	  		rocketMq or activeMq
	  	cache
	  		memcache or redis or ehcache
  	</description>
  	
  	<properties>
  		<spring.version>4.3.2</spring.version>
  		<spring.security.version>4.1.3</spring.security.version>
  		<spring.oauth2.version>2.0.11</spring.oauth2.version>
		<mybatis.version>3.4.1</mybatis.version>
		<mybatis.spring.version>1.3.0</mybatis.spring.version>
  	</properties>
  	
  	<repositories>
  		<repository>
  			<id>myNexus.release</id>
  			<name>myNexus.release</name>
			<releases>
				<enabled>true</enabled>
			</releases>
  			<url>http://119.253.134.199:8097/nexus/content/repositories/releases</url>
  		</repository>
  		
  		<repository>
  			<id>myNexus.snapshots</id>
  			<name>myNexus.snapshots</name>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
  			<url>http://119.253.134.199:8097/nexus/content/repositories/snapshots</url>
  		</repository>
  	</repositories>

	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.11</version>
			<scope>test</scope>
		</dependency>
		
		
		
	</dependencies>
	<build>
		<finalName>java-sm</finalName>
	</build>
</project>
